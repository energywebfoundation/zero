{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "zero-ui",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "strict": true
      }
    }
  },
  "projects": {
    "account": {
      "root": "libs/account",
      "sourceRoot": "libs/account/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/account",
            "tsConfig": "libs/account/tsconfig.lib.json",
            "project": "libs/account/package.json",
            "entryFile": "libs/account/src/index.ts",
            "external": ["react/jsx-runtime", "@emotion/styled/base"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/account/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/account/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/account"],
          "options": {
            "jestConfig": "libs/account/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-client": {
      "root": "libs/api-client",
      "sourceRoot": "libs/api-client/src",
      "projectType": "library",
