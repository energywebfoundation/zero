            "tsConfig": "libs/ui/forms/tsconfig.lib.json",
            "project": "libs/ui/forms/package.json",
            "entryFile": "libs/ui/forms/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
