          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/assets"],
          "options": {
            "jestConfig": "libs/ui/assets/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui-core": {
      "root": "libs/ui/core",
      "sourceRoot": "libs/ui/core/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/ui/core",
            "tsConfig": "libs/ui/core/tsconfig.lib.json",
            "project": "libs/ui/core/package.json",
            "entryFile": "libs/ui/core/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "rollup.config.js",
            "assets": [
              {
                "glob": "libs/ui/core/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/core/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/core"],
          "options": {
            "jestConfig": "libs/ui/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui/core/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/ui-core",
            "config": {
              "configFolder": "libs/ui/core/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ui-localization": {
      "root": "libs/ui/localization",
      "sourceRoot": "libs/ui/localization/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/localization/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/localization"],
