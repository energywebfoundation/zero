          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/theme"],
          "options": {
            "jestConfig": "libs/ui/theme/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "zero-api": {
      "root": "apps/zero-api",
      "sourceRoot": "apps/zero-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/zero-api",
            "main": "apps/zero-api/src/main.ts",
            "tsConfig": "apps/zero-api/tsconfig.app.json",
            "assets": ["apps/zero-api/src/assets"],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/zero-api/src/environments/environment.ts",
                  "with": "apps/zero-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "docker-image-build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              "nx r zero-api:build:production",
              "cp -R ./apps/zero-api/prisma ./dist/apps/zero-api/prisma",
              "cp ./apps/zero-api/Dockerfile ./dist/apps/zero-api/",
              "docker tag zero-api:latest zero-api:previous || exit 0",
              "docker build ./dist/apps/zero-api/ -t zero-api",
              "rm -r ./dist/apps/zero-api/prisma ./dist/apps/zero-api/Dockerfile"
            ]
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "zero-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/zero-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/zero-api"],
          "options": {
            "jestConfig": "apps/zero-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "zero-ui": {
      "root": "apps/zero-ui",
      "sourceRoot": "apps/zero-ui/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/zero-ui",
            "index": "apps/zero-ui/src/index.html",
            "main": "apps/zero-ui/src/main.tsx",
            "polyfills": "apps/zero-ui/src/polyfills.ts",
            "tsConfig": "apps/zero-ui/tsconfig.app.json",
            "assets": [
              "apps/zero-ui/src/favicon.ico",
              "apps/zero-ui/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/zero-ui/src/environments/environment.ts",
                  "with": "apps/zero-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "2.5mb"
                }
              ]
            }
          }
        },
        "docker-image-build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              "nx r zero-ui:build:production",
              "cp ./apps/zero-ui/Dockerfile ./dist/apps/zero-ui/",
              "cp -R ./apps/zero-ui/bin ./dist/apps/zero-ui/bin",
              "cp -R ./apps/zero-ui/nginx ./dist/apps/zero-ui/nginx",
              "docker tag zero-ui:latest zero-ui:previous || exit 0",
              "docker build ./dist/apps/zero-ui/ -t zero-ui",
              "rm -r ./dist/apps/zero-ui/Dockerfile ./dist/apps/zero-ui/bin ./dist/apps/zero-ui/nginx"
            ]
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "zero-ui:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "zero-ui:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/zero-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/zero-ui"],
          "options": {
            "jestConfig": "apps/zero-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "zero-ui-e2e": {
      "root": "apps/zero-ui-e2e",
      "sourceRoot": "apps/zero-ui-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/zero-ui-e2e/cypress.json",
            "tsConfig": "apps/zero-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "zero-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "zero-ui:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/zero-ui-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
