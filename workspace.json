        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/buyer/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/buyer"],
          "options": {
            "jestConfig": "libs/buyer/jest.config.js",
            "passWithNoTests": true
