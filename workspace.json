      }
    },
    "ui-auth": {
      "root": "libs/ui/auth",
      "sourceRoot": "libs/ui/auth/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
