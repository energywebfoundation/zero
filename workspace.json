          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/zero-api"],
          "options": {
            "jestConfig": "libs/ui/zero-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "zero": {
      "root": "apps/zero",
      "sourceRoot": "apps/zero/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/zero",
            "index": "apps/zero/src/index.html",
            "main": "apps/zero/src/main.tsx",
            "polyfills": "apps/zero/src/polyfills.ts",
            "tsConfig": "apps/zero/tsconfig.app.json",
            "assets": ["apps/zero/src/favicon.ico", "apps/zero/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/zero/src/environments/environment.ts",
                  "with": "apps/zero/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "docker-image-build" : {
          "builder": "@nrwl/workspace:run-commands",
          "options" : {
            "parallel" : false,
            "commands" : [
              "nx build zero",
              "cp ./apps/zero/Dockerfile ./dist/apps/zero/",
              "cp -R ./apps/zero/bin ./dist/apps/zero/bin",
              "cp -R ./apps/zero/nginx ./dist/apps/zero/nginx",
              "docker tag zero-ui:latest zero-ui:previous",
              "docker build ./dist/apps/zero/ -t zero-ui",
              "rm -r ./dist/apps/zero/Dockerfile ./dist/apps/zero/bin ./dist/apps/zero/nginx"
            ]
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "zero:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "zero:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/zero/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/zero"],
          "options": {
            "jestConfig": "apps/zero/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "zero-api": {
      "root": "apps/zero-api",
      "sourceRoot": "apps/zero-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/zero-api",
            "main": "apps/zero-api/src/main.ts",
            "tsConfig": "apps/zero-api/tsconfig.app.json",
            "assets": ["apps/zero-api/src/assets"],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/zero-api/src/environments/environment.ts",
                  "with": "apps/zero-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "docker-image-build" : {
          "builder": "@nrwl/workspace:run-commands",
          "options" : {
            "parallel" : false,
            "commands" : [
              "nx build zero-api",
              "cp -R ./prisma ./dist/apps/zero-api/prisma",
              "cp ./apps/zero-api/Dockerfile ./dist/apps/zero-api/",
              "docker tag zero-api:latest zero-api:previous",
              "docker build ./dist/apps/zero-api/ -t zero-api",
              "rm -r ./dist/apps/zero-api/prisma ./dist/apps/zero-api/Dockerfile"
            ]
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "zero-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/zero-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/zero-api"],
          "options": {
            "jestConfig": "apps/zero-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "zero-e2e": {
      "root": "apps/zero-e2e",
      "sourceRoot": "apps/zero-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/zero-e2e/cypress.json",
            "tsConfig": "apps/zero-e2e/tsconfig.e2e.json",
            "devServerTarget": "zero:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "zero:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/zero-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "zero-ui-assets": {
      "root": "libs/zero-ui-assets",
      "sourceRoot": "libs/zero-ui-assets/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/zero-ui-assets/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/zero-ui-assets"],
          "options": {
            "jestConfig": "libs/zero-ui-assets/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
