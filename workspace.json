            "tsConfig": "libs/ui/auth/tsconfig.lib.json",
            "project": "libs/ui/auth/package.json",
            "entryFile": "libs/ui/auth/src/index.ts",
            "external": ["react/jsx-runtime", "@emotion/styled/base"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
