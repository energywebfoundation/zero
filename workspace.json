    "seller": {
      "root": "libs/seller",
      "sourceRoot": "libs/seller/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/seller",
            "tsConfig": "libs/seller/tsconfig.lib.json",
            "project": "libs/seller/package.json",
            "entryFile": "libs/seller/src/index.ts",
            "external": ["react/jsx-runtime", "@emotion/styled/base"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
