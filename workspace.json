          "options": {
            "lintFilePatterns": ["libs/ui/utils/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/utils"],
          "options": {
            "jestConfig": "libs/ui/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui-zero-api": {
      "root": "libs/ui/zero-api",
      "sourceRoot": "libs/ui/zero-api/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/ui/zero-api",
            "tsConfig": "libs/ui/zero-api/tsconfig.lib.json",
            "project": "libs/ui/zero-api/package.json",
            "entryFile": "libs/ui/zero-api/src/index.ts",
