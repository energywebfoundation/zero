  "info": {
    "title": "Energy Web Zero API",
    "description": "",
    "version": "0.3",
    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "http://localhost:3333"
    },
    {
      "url": "http://zero-dev.energyweb.org"
    },
    {
      "url": "https://zero-dev.energyweb.org"
    }
  ],
  "components": {
    "securitySchemes": {
      "access-token": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "LoginDataDTO": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "example": "testuser1@foo.bar"
          },
          "password": {
            "type": "string",
            "example": "test"
          }
        },
        "required": [
          "username",
          "password"
        ]
      },
      "LoginResponseDataDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          }
        },
        "required": [
          "accessToken"
        ]
      },
      "UserRole": {
        "type": "string",
        "enum": [
          "seller",
          "buyer",
          "admin"
        ]
      },
      "UserDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": 1
          },
          "email": {
            "type": "string",
            "example": "testuser1@foo.bar"
          },
          "firstName": {
            "type": "string",
            "example": "John"
          },
          "lastName": {
            "type": "string",
            "example": "Smith"
          },
          "roles": {
