{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "develop"
  },
  "npmScope": "energy-web-zero",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "account": {
      "tags": []
    },
    "api-client": {
      "tags": []
    },
    "auth": {
      "tags": []
    },
    "buyer": {
      "tags": []
    },
    "seller": {
      "tags": []
    },
    "store": {
      "tags": []
    },
    "ui-assets": {
      "tags": []
    },
    "ui-core": {
      "tags": []
    },
    "ui-localization": {
      "tags": []
    },
    "ui-theme": {
      "tags": []
    },
    "zero-api": {
      "tags": []
    },
    "zero-ui": {
      "tags": []
    },
    "zero-ui-e2e": {
      "tags": [],
      "implicitDependencies": ["zero-ui"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
